zip zipFile: 'artifacts/<%= job_name %>-prefix.zip', archive: true, glob: "<%= artifact_glob %>"

def triggered_by_user = (currentBuild.rawBuild.getCause(hudson.model.Cause$UserIdCause) != null)
if (triggered_by_user) {
    <% downstream_jobs.each do |job_name| %>
    build job: "<%= job_name %>", wait: false
    <% end %>
}

